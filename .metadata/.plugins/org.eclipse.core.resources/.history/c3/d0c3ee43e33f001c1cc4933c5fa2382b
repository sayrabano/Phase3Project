	<script>
	
	function deleteProduct(product_id){
		
		
		
		swal({
			  title: "Are you sure?",
			  text: "You want to delete this product..!",
			  icon: "warning",
			  buttons: true,
			  dangerMode: true,
			})
			.then((willDelete) => {
			  if (willDelete) {
				  window.location="/manageProduct/"+product_id;
			    
			   
			  } else {
			    swal("Your product is safe!!");
			  }
			});
	}
	
	</script>

